{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak" : {
    "realm": "my-app",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "front-end-app",
    "verify-token-audience": true,
    "credentials": {
      "secret": "<front-end-app-user-secret>"
    },
    "confidential-port": 0
  }
}
